 356  ***--------------------------------------------------------------------------***
 357  ***                          SPOT MARKET MODEL                               ***
 358  ***--------------------------------------------------------------------------***
 359   
 360  *** Objective function
 361    Equation welfSpot;
 362    welfSpot..         welfareSpot =e=
 363    (1-weight_sp)*
 364                                          sum((S_trans,S_co2,S_dloc,S_dlev), prob_trans(S_trans)*prob_co2(S_co2)*prob_dloc(S_dloc)*prob_dlev(S_dlev)
 365                                        * ( sum((D,T), periodScale(T)*( consObjA(D,T,S_dlev,S_dloc) * d_sp(S_trans,S_co2,S_dloc,S_dlev,D,T)
 366                                        - 0.5 * consObjB(D,T,S_dlev,S_dloc) * d_sp(S_trans,S_co2,S_dloc,S_dlev,D,T) * d_sp(S_trans,S_co2,S_dloc,S_dlev,D,T) ) )
 367                                        - sum((G,T), genVarInv(G,S_co2) * g_sp(S_trans,S_co2,S_dloc,S_dlev,G,T) * periodScale(T) ) ) ) * Year
 368                                        - sum(G, genFixInv(G) * ig_sp(G) )
 369                                        + weight_sp*(VAR_sp - (1/(1-percentile)
 370                                        *sum((S_trans,S_co2,S_dloc,S_dlev), prob_trans(S_trans)*prob_co2(S_co2)*prob_dloc(S_dloc)*prob_dlev(S_dlev)
 371                                        * eta_sp(S_trans,S_co2,S_dloc,S_dlev))))
 372  ;
 373   
 374  *** Conditional Value at Risk Restrictions ***
 375    Equation CVARSpot;
 376    CVARSpot(S_trans,S_co2,S_dloc,S_dlev)..
 377          VAR_sp - (( sum((D,T), periodScale(T)*( consObjA(D,T,S_dlev,S_dloc) * d_sp(S_trans,S_co2,S_dloc,S_dlev,D,T)
 378                  - 0.5 * consObjB(D,T,S_dlev,S_dloc) * d_sp(S_trans,S_co2,S_dloc,S_dlev,D,T) * d_sp(S_trans,S_co2,S_dloc,S_dlev,D,T) ) )
 379                  - sum((G,T), genVarInv(G,S_co2) * g_sp(S_trans,S_co2,S_dloc,S_dlev,G,T) * periodScale(T) ) )  * Year
 380                  - sum(G, genFixInv(G) * ig_sp(G))) =l=eta_sp(S_trans,S_co2,S_dloc,S_dlev)
 381                  ;
 382   
 383   
 384  *** Zonal First Kirchhoffs Law
 385   
 386    Equation ZFKL;
 387    ZFKL(S_trans,S_co2,S_dloc,S_dlev,Z,T)..
 388           sum(D$(consInZone(D) = Z.val), d_sp(S_trans,S_co2,S_dloc,S_dlev,D,T)) =e=
 389                       sum(G$(sum(N$(genAtNode(G) = N.val), NodeInZone(N)) = Z.val), g_sp(S_trans,S_co2,S_dloc,S_dlev,G,T) )
 390                     - sum(L$(sum(N$(lineStart(L) = N.val), NodeInZone(N)) = Z.val and lineInter(L) = 1), f_sp(S_trans,S_co2,S_dloc,S_dlev,L,T))
 391                     + sum(L$(sum(N$(lineEnd(L) = N.val),   NodeInZone(N)) = Z.val and lineInter(L) = 1), f_sp(S_trans,S_co2,S_dloc,S_dlev,L,T)) ;
 392   
 393   
 394  *** Market Coupling Flow Restrictions
 395   
 396    Equation MCF1;
 397    MCF1(S_trans,S_co2,S_dloc,S_dlev,L,T)$(lineInter(L) = 1 and lineIsNew(L) = 0)..   f_sp(S_trans,S_co2,S_dloc,S_dlev,L,T) =l= lineUB(L);
 398    Equation MCF2;
 399    MCF2(S_trans,S_co2,S_dloc,S_dlev,L,T)$(lineInter(L) = 1 and lineIsNew(L) = 0).. - lineUB(L)=l= f_sp(S_trans,S_co2,S_dloc,S_dlev,L,T);
 400    Equation MCF3;
 401    MCF3(S_trans,S_co2,S_dloc,S_dlev,L,T)$(lineInter(L) = 1 and lineIsNew(L) = 1)..   f_sp(S_trans,S_co2,S_dloc,S_dlev,L,T) =l= lineB(L) * lineUB(L);
 402    Equation MCF4;
 403    MCF4(S_trans,S_co2,S_dloc,S_dlev,L,T)$(lineInter(L) = 1 and lineIsNew(L) = 1).. - lineB(L) * lineUB(L) =l= f_sp(S_trans,S_co2,S_dloc,S_dlev,L,T);
 404   
 405   
 406   
 407  ***Generation Capacity Limits
 408   
 409    Equation GCLSpot ;
 410    GCLSpot(S_trans,S_co2,S_dloc,S_dlev,G,T)..  g_sp(S_trans,S_co2,S_dloc,S_dlev,G,T) =l= avail(T,G) * ig_sp(G) ;
 411   
 412   
 413  ***--------------------------------------------------------------------------***
 414  ***                     NETWORK- and REDISPATCH LEVEL                        ***
 415  ***--------------------------------------------------------------------------***
 416   
 417    Equation costRed ;
 418    costRed..         costRedispatch =e= (1-weight_rd)
 419                                             *sum((S_trans,S_co2,S_dloc,S_dlev), prob_trans(S_trans)*prob_co2(S_co2)*prob_dloc(S_dloc)*prob_dlev(S_dlev)
 420                                             * ( sum((G,T), genVarInv(G,S_co2) * ( g_rd(S_trans,S_co2,S_dloc,S_dlev,G,T) - SP_GEN_G(S_trans,S_co2,S_dloc,S_dlev,G,T) )
 421                                             * periodScale(T) ) * YEAR
 422                                             + sum((B,T), buVarInv(S_co2) * gb_rd(S_trans,S_co2,S_dloc,S_dlev,B,T) * periodScale(T) ) * YEAR )
 423                                             + sum(L$(lineIsNew(L) = 1), lineFixInv(L,S_trans) * lineB(L)))
 424                                             + sum(B, buFixInv * ib_rd(B) )
 425                                             + weight_rd*CVAR
 426                                             ;
 427   
 428   
 429  *** Conditional Value at Risk Restrictions ***
 430   
 431    Equation CVARRed1;
 432    CVARRed1..
 433           (VAR_rd + (1/(1-percentile)
 434           *sum((S_trans,S_co2,S_dloc,S_dlev), prob_trans(S_trans)*prob_co2(S_co2)*prob_dloc(S_dloc)*prob_dlev(S_dlev)
 435           * eta_rd(S_trans,S_co2,S_dloc,S_dlev)))) =l= CVAR
 436   
 437    Equation CVARRed2;
 438    CVARRed2(S_trans,S_co2,S_dloc,S_dlev)..
